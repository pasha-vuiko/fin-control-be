{
  "name": "fin-control-be",
  "version": "1.0.0",
  "private": true,
  "description": "BE for self finances tracking application",
  "license": "UNLICENSED",
  "author": "",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "sonar": "node --env-file=.env sonar-scanner.mjs",
    "prisma:deploy": "npx prisma migrate deploy",
    "prisma:generate": "npx prisma generate",
    "start": "node --env-file=.env dist/main.js",
    "start:bun": "bun run dist/main.js",
    "start:debug": "nest start --debug --watch -e 'node --env-file=.env'",
    "start:dev": "nest start --watch -e 'node --env-file=.env'",
    "start:prod": "node dist/main",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --no-file-parallelism",
    "test:e2e": "vitest run --config ./test/vitest.config.e2e.mjs",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@fastify/static": "^7.0.3",
    "@fastify/swagger": "^8.14.0",
    "@geut/fastify-uws": "^3.1.1",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.3.8",
    "@nestjs/core": "^10.3.8",
    "@nestjs/platform-fastify": "^10.3.8",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/swagger": "^7.3.1",
    "@prisma/client": "^5.13.0",
    "cache-manager": "^5.5.2",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "fastify": "^4.26.2",
    "fastify-auth0-verify": "^2.1.0",
    "fastify-metrics": "^11.0.0",
    "fastify-plugin": "^4.5.1",
    "ioredis": "^5.4.1",
    "pino": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "prisma-error-enum": "^0.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.8",
    "@semantic-release/git": "^10.0.1",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.5.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/cache-manager": "^4.0.6",
    "@types/cache-manager-ioredis": "^2.0.6",
    "@types/node": "^20.12.7",
    "@types/supertest": "^6.0.2",
    "@types/validator": "^13.11.9",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vitest/coverage-v8": "^1.5.2",
    "eslint": "^9.1.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-security": "^3.0.0",
    "prettier": "^3.2.5",
    "prisma": "^5.13.0",
    "semantic-release": "^23.0.8",
    "sonarqube-scanner": "^3.5.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "swc-loader": "^0.2.6",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "unplugin-swc": "^1.4.5",
    "vitest": "^1.5.2"
  },
  "engines": {
    "node": "20.12.2"
  },
  "overrides": {
    "@typescript-eslint/eslint-plugin": {
      "eslint": "^9.1.1"
    }
  }
}
