{
  "name": "fin-control-be",
  "version": "0.7.0",
  "private": true,
  "description": "BE for self finances tracking application",
  "license": "UNLICENSED",
  "author": "",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prisma:deploy": "npx prisma migrate deploy",
    "prisma:generate": "npx prisma generate",
    "start": "node --env-file=.env dist/main.js",
    "start:bun": "bun run dist/main.js",
    "start:debug": "nest start --debug --watch -e 'node --env-file=.env'",
    "start:dev": "nest start --watch -e 'node --env-file=.env'",
    "start:prod": "node dist/main",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "vitest run --config ./test/vitest.config.e2e.mjs"
  },
  "dependencies": {
    "@fastify/static": "^7.0.1",
    "@fastify/swagger": "^8.14.0",
    "@nestjs/cache-manager": "^2.2.1",
    "@nestjs/common": "^10.3.3",
    "@nestjs/core": "^10.3.3",
    "@nestjs/platform-fastify": "^10.3.3",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/swagger": "^7.3.0",
    "@prisma/client": "^5.10.2",
    "cache-manager": "^5.4.0",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.1",
    "fastify-auth0-verify": "^2.0.0",
    "fastify-plugin": "^4.5.1",
    "fastify-uws": "^0.5.0",
    "ioredis": "^5.3.2",
    "pino": "^8.19.0",
    "prisma-error-enum": "^0.1.3",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.3",
    "@swc/cli": "^0.3.10",
    "@swc/core": "^1.4.2",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/cache-manager": "^4.0.6",
    "@types/cache-manager-ioredis": "^2.0.6",
    "@types/cron": "^2.0.1",
    "@types/node": "^20.11.24",
    "@types/supertest": "^6.0.2",
    "@types/validator": "^13.11.9",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vitest/coverage-v8": "^1.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-security": "^2.1.1",
    "pino-pretty": "^10.3.1",
    "prettier": "^3.2.5",
    "prisma": "^5.10.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "swc-loader": "^0.2.6",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "unplugin-swc": "^1.4.4",
    "vitest": "^1.3.1"
  }
}
