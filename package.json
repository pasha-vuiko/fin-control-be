{
  "name": "fin-control-be",
  "version": "1.0.0",
  "private": true,
  "description": "BE for self finances tracking application",
  "license": "UNLICENSED",
  "author": "",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migrations:run": "drizzle-kit migrate",
    "sonar": "node --env-file=.env sonar-scanner.mjs",
    "start": "node --env-file=.env dist/main.js",
    "start:bun": "bun run dist/main.js",
    "start:debug": "nest start --debug --watch -e 'node --env-file=.env'",
    "start:dev": "nest start --watch -e 'node --env-file=.env'",
    "start:prod": "node --enable-source-maps dist/main",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --no-file-parallelism",
    "test:e2e": "vitest run --config ./test/vitest.config.e2e.mjs",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@fastify/static": "^7.0.4",
    "@fastify/swagger": "^8.14.0",
    "@fastify/under-pressure": "^8.5.1",
    "@geut/fastify-uws": "^5.0.1",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.3.10",
    "@nestjs/core": "^10.3.10",
    "@nestjs/platform-fastify": "^10.3.10",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/swagger": "^7.4.0",
    "async-cache-dedupe": "^2.2.0",
    "cache-manager": "^5.7.2",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "drizzle-orm": "^0.32.0",
    "fast-jwt": "^4.0.1",
    "fastify": "^4.28.1",
    "fastify-metrics": "^11.0.0",
    "fastify-plugin": "^4.5.1",
    "get-jwks": "^9.0.2",
    "ioredis": "^5.4.1",
    "pino": "^9.2.0",
    "pino-pretty": "^11.2.1",
    "postgres": "^3.4.4",
    "prisma-error-enum": "^0.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@nestjs/cli": "^10.4.2",
    "@nestjs/schematics": "^10.1.2",
    "@nestjs/testing": "^10.3.10",
    "@semantic-release/git": "^10.0.1",
    "@swc/core": "^1.6.13",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/cache-manager": "^4.0.6",
    "@types/cache-manager-ioredis": "^2.0.6",
    "@types/eslint__js": "^8.42.3",
    "@types/k6": "^0.52.0",
    "@types/node": "^20.14.10",
    "@types/supertest": "^6.0.2",
    "@types/validator": "^13.12.0",
    "@vitest/coverage-v8": "^2.0.2",
    "drizzle-kit": "^0.23.0",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-security": "^3.0.1",
    "prettier": "^3.3.3",
    "semantic-release": "^24.0.0",
    "sonarqube-scanner": "^4.0.1",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.0",
    "unplugin-swc": "^1.5.1",
    "vitest": "^2.0.2"
  },
  "engines": {
    "node": "20.15.0"
  },
  "overrides": {
    "typescript-eslint": {
      "eslint": "^9.7.0"
    },
    "@nestjs/platform-fastify": {
      "fastify": "4.28.1"
    }
  }
}
